<h1>Listing contact_numbers</h1>

<table>
  <thead>
    <tr>
      <th>Phone number</th>
      <th>Type</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_numbers.each do |contact_number| %>
      <tr>
        <td><%= contact_number.phone_number %></td>
        <td><%= contact_number.contact_type %></td>
        <td><%= contact_number.person_id %></td>
        <td><%= link_to 'Show', contact_number %></td>
        <td><%= link_to 'Edit', edit_contact_number_path(contact_number) %></td>
        <td><%= link_to 'Destroy', contact_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact number', new_contact_number_path %>
